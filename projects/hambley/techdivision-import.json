{
  "magento-edition": "CE",
  "magento-version": "2.3.0",
  "operation-name" : "add-update",
  "archive-artefacts" : false,
  "debug-mode" : false,
  "source-dir" : "projects/hambley/tmp",
  "target-dir" : "projects/hambley/tmp",
  "entity-type-code" : "catalog_product",
  "listeners" : [
    {
      "app.set.up" : [
        "import.listener.render.ansi.art",
        "import.listener.initialize.registry"
      ]
    },
    {
      "app.tear.down" : [
        "import.listener.clear.registry"
      ]
    }
  ],
  "databases" : [
    {
      "id": "docker",
      "default" : true,
      "pdo-dsn": "mysql:host=127.0.1.4:9306;dbname=magento;charset=utf8",
      "username": "magento",
      "password": "magento"
    }
  ],
  "loggers": [
    {
      "name": "system",
      "channel-name": "logger/system",
      "type": "Monolog\\Logger",
      "handlers": [
        {
          "type": "Monolog\\Handler\\ErrorLogHandler",
          "formatter": {
            "type": "Monolog\\Formatter\\LineFormatter",
            "params" : [
              {
                "format": "[%datetime%] %channel%.%level_name%: %message% %context% %extra%",
                "date-format": "Y-m-d H:i:s",
                "allow-inline-line-breaks": true,
                "ignore-empty-context-and-extra": false
              }
            ]
          }
        }
      ]
    }
  ],
  "operations" : [
    {
      "name" : "replace",
      "plugins" : [
        {
          "id": "import.plugin.cache.warmer"
        },
        {
          "id": "import.plugin.global.data"
        },
        {
          "id": "import.plugin.subject",
          "subjects": [
            {
              "id": "import.subject.move.files",
              "identifier": "move-files",
              "file-resolver": {
                "prefix": "product-import"
              },
              "ok-file-needed": true
            },
            {
              "id": "import_product.subject.bunch",
              "identifier": "files",
              "file-resolver": {
                "prefix": "product-import"
              },
              "filesystem-adapter" : {
                "id" : "import.adapter.filesystem.factory.league",
                "adapter" : {
                  "type" : "League\\Flysystem\\Adapter\\Local"
                }
              },
              "params" : [
                {
                  "copy-images" : false,
                  "media-directory" : "projects/sample-data/tmp",
                  "images-file-directory" : "projects/sample-data/magento2-sample-data/pub/media/catalog/product"
                }
              ],
              "observers": [
                {
                  "import": [
                    "import_product.observer.composite.base.replace"
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "import.plugin.archive"
        }
      ]
    },
    {
      "name" : "add-update",
      "plugins" : [
        {
          "id": "import.plugin.cache.warmer",
          "params" : [
            {
              "cache-warmers" : [
                "import_product.repository.cache.warmer.product"
              ]
            }
          ]
        },
        {
          "id": "import.plugin.global.data"
        },
        {
          "id": "import.plugin.subject",
          "subjects": [{
              "id": "import.subject.move.files",
              "identifier": "move-files",
              "file-resolver": {
                "prefix": "product-import"
              },
              "ok-file-needed": true
            },
            {
              "id": "import_product.subject.bunch",
              "identifier": "files",
              "file-resolver": {
                "prefix": "product-import"
              },
              "params" : [
                {
                  "copy-images" : true,
                  "clean-up-media-gallery" : true,
                  "clean-up-empty-columns" : [ "base_image", "small_image", "swatch_image", "thumbnail_image" ],
                  "media-directory" : "projects/hambley/tmp",
                  "images-file-directory" : "projects/hambley/images"
                }
              ],
              "observers": [
                {
                  "import": [
                    "import.observer.attribute.set",
                    "import_product.observer.file.upload",
                    "import_product.observer.last.entity.id",
                    "import_product.observer.product.attribute.update",
                    "import_product_media.observer.product.media",
                    "import_product_media.observer.clear.media.gallery",
                    "import_product.observer.clean.up"
                  ]
                }
              ]
            },
            {
              "id": "import_product_media.subject.media",
              "identifier": "files",
              "file-resolver": {
                "prefix": "media"
              },
              "observers": [
                {
                  "import": [
                    "import.observer.attribute.set",
                    "import_product_media.observer.media.gallery.update",
                    "import_product_media.observer.media.gallery.value.update"
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "import.plugin.archive"
        }
      ]
    }
  ]
}