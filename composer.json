        "doctrine/annotations" : "1.6.0",
        "doctrine/instantiator" : "1.1.0",
        "jms/serializer": "1.13.0",
        "monolog/monolog": "~1.0",
        "league/flysystem": "1.0.40",
        "symfony/config": "~4.0",
        "symfony/console": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "techdivision/import": "4.0.*",
        "techdivision/import-ee": "3.0.*",
        "techdivision/import-app-simple": "4.0.*",
        "techdivision/import-configuration-jms": "4.0.*",
        "techdivision/import-attribute": "3.0.*",
        "techdivision/import-category": "3.0.*",
        "techdivision/import-category-ee": "3.0.*",
        "techdivision/import-product": "3.0.*",
        "techdivision/import-product-url-rewrite": "3.0.*",
        "techdivision/import-product-link": "3.0.*",
        "techdivision/import-product-media": "3.0.*",
        "techdivision/import-product-bundle": "3.0.*",
        "techdivision/import-product-variant": "3.0.*",
        "techdivision/import-product-ee": "3.0.*",
