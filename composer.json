        "techdivision/import-product-media-ee": "3.0.*",
        "techdivision/import-product-bundle-ee": "3.0.*",
        "techdivision/import-product-variant-ee": "3.0.*"
    },
    "require-dev": {
        "doctrine/dbal": "2.5.*",
        "pdepend/pdepend": "2.5.2",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~5.0",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "3.3.1",
        "consolidation/robo": "~1.0",
        "mikey179/vfsStream": "~1.0",
        "symfony/http-kernel": "~3.4"
    },
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "t.wagner@techdivision.com"
