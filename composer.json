{
    "name": "techdivision/import-cli-simple",
    "description": "CLI providing single-threaded Magento 2 importing functionality based on M2IF",
    "license": "OSL-3.0",
    "require": {
        "php": ">=7.1.0",
        "ramsey/uuid": "3.8.*",
        "doctrine/cache" : "1.6.2",
        "doctrine/dbal": "2.5.13",
        "doctrine/lexer" : "1.0.1",
        "doctrine/common" : "2.7.3",
        "doctrine/inflector" : "1.1.0",
