        "doctrine/lexer" : "1.0.1",
        "doctrine/common" : "2.7.3",
        "doctrine/inflector" : "1.1.0",
        "doctrine/collections" : "1.4.0",
        "doctrine/annotations" : "1.4.0",
        "doctrine/instantiator" : "1.0.5",
        "jms/serializer": "1.8.1",
        "monolog/monolog": "~1.0",
        "league/flysystem": "1.0.40",
        "symfony/config": "~4.0",
        "symfony/console": "~4.0",
        "symfony/yaml": "~4.0",
        "symfony/expression-language": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "techdivision/import": "3.0.*",
        "techdivision/import-ee": "2.0.*",
        "techdivision/import-app-simple": "compat/magento23-dev",
        "techdivision/import-configuration-jms": "3.0.*",
        "techdivision/import-attribute": "2.0.*",
        "techdivision/import-category": "2.0.*",
        "techdivision/import-category-ee": "2.0.*",
        "techdivision/import-product": "2.0.*",
        "techdivision/import-product-url-rewrite": "2.0.*",
        "techdivision/import-product-link": "2.0.*",
        "techdivision/import-product-media": "2.0.*",
        "techdivision/import-product-bundle": "2.0.*",
        "techdivision/import-product-variant": "2.0.*",
        "techdivision/import-product-ee": "2.0.*",
        "techdivision/import-product-link-ee": "2.0.*",
        "techdivision/import-product-media-ee": "2.0.*",
        "techdivision/import-product-bundle-ee": "2.0.*",
        "techdivision/import-product-variant-ee": "2.0.*"
    },
    "require-dev": {
        "phpmd/phpmd": "2.5.*",
        "phpunit/phpunit": "4.4.*",
        "sebastian/phpcpd": "2.0.*",
        "symfony/http-kernel": "~2.8|~3.0",
        "consolidation/log": "~1.0",
        "consolidation/robo": "~1.0",
        "squizlabs/php_codesniffer": "2.2.*",
        "tomzx/php-semver-checker": "0.12.*",
        "mikey179/vfsStream": "~1.0"
    },
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "t.wagner@techdivision.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TechDivision\\Import\\Cli\\": [
                "src/",
                "symfony/",
                "tests/unit/",
                "tests/integration/"
            ]
        }
    },
    "bin": [
        "bin/import-simple",
        "bin/import-simple.php"
    ]
